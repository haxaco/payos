{
  "name": "@payos/cards",
  "version": "0.1.0",
  "description": "Card network integration for Visa VIC and Mastercard Agent Pay",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./visa": {
      "types": "./dist/visa/index.d.ts",
      "import": "./dist/visa/index.mjs",
      "require": "./dist/visa/index.js"
    },
    "./mastercard": {
      "types": "./dist/mastercard/index.d.ts",
      "import": "./dist/mastercard/index.mjs",
      "require": "./dist/mastercard/index.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@noble/ed25519": "^2.1.0",
    "@noble/hashes": "^1.5.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.3",
    "vitest": "^1.0.0"
  },
  "peerDependencies": {
    "@payos/types": "workspace:*"
  }
}
