{
  "name": "@payos/sdk",
  "version": "0.1.0",
  "description": "Unified PayOS SDK for multi-protocol settlement in LATAM",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./x402": {
      "types": "./dist/x402.d.ts",
      "import": "./dist/x402.mjs",
      "require": "./dist/x402.js"
    },
    "./ap2": {
      "types": "./dist/ap2.d.ts",
      "import": "./dist/ap2.mjs",
      "require": "./dist/ap2.js"
    },
    "./acp": {
      "types": "./dist/acp.d.ts",
      "import": "./dist/acp.mjs",
      "require": "./dist/acp.js"
    },
    "./ucp": {
      "types": "./dist/ucp.d.ts",
      "import": "./dist/ucp.mjs",
      "require": "./dist/ucp.js"
    },
    "./langchain": {
      "types": "./dist/langchain.d.ts",
      "import": "./dist/langchain.mjs",
      "require": "./dist/langchain.js"
    },
    "./vercel": {
      "types": "./dist/vercel.d.ts",
      "import": "./dist/vercel.mjs",
      "require": "./dist/vercel.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "payos",
    "payments",
    "settlement",
    "x402",
    "ap2",
    "acp",
    "ucp",
    "latam",
    "pix",
    "spei",
    "stablecoin",
    "usdc",
    "agentic-commerce"
  ],
  "dependencies": {
    "@payos/types": "workspace:*",
    "@x402/core": "^2.1.0",
    "@x402/evm": "^2.1.0",
    "@x402/express": "^2.1.0",
    "@x402/fetch": "^2.1.0",
    "ai": "^3.0.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
    "@vitest/coverage-v8": "^2.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.3",
    "vitest": "^2.0.0"
  },
  "peerDependencies": {
    "express": "^4.18.0"
  },
  "peerDependenciesMeta": {
    "express": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=18"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/payos/payos",
    "directory": "packages/sdk"
  },
  "author": "PayOS",
  "license": "MIT"
}
