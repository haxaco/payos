{
  "name": "@sly/scanner",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "dev:mcp": "tsx src/mcp/server.ts",
    "build": "tsup src/index.ts src/mcp/server.ts --format esm --dts",
    "start": "node dist/index.js",
    "start:mcp": "node dist/mcp/server.js",
    "seed": "tsx src/cli.ts seed",
    "lint": "eslint src/",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@sly/types": "workspace:*",
    "@sly/utils": "workspace:*",
    "@supabase/supabase-js": "^2.47.10",
    "@hono/node-server": "^1.13.7",
    "hono": "^4.6.14",
    "zod": "^3.24.1",
    "@modelcontextprotocol/sdk": "^1.12.1",
    "cheerio": "^1.0.0",
    "undici": "^7.3.0",
    "p-limit": "^6.2.0",
    "csv-parse": "^5.6.0",
    "dotenv": "^17.2.3"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "tsup": "^8.3.5",
    "tsx": "^4.19.2",
    "typescript": "^5.3.3",
    "vitest": "^4.0.15"
  }
}
